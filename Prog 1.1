using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication4
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Введите длину стороны, l: ");
            Double l = Convert.ToDouble(Console.ReadLine());
            Console.Write("Введите ширину стороны, w: ");
            Double w = Convert.ToDouble(Console.ReadLine());
            Console.Write("Введите диагональ стороны, d: ");
            Double d = Convert.ToDouble(Console.ReadLine());
            Double h = Math.Sqrt((d * d) - (l * l));
            Double V = Math.Sqrt(l*l * w*w * h*h);
            Console.WriteLine(String.Format("Объем равен: {0}", V));
            Console.ReadLine();
        }
    }
}
